{
	"name": "homepage",
	"version": "1.2.0",
	"description": "Ross Hill's homepage",
	"scripts": {
		"build": "rm -rf .svelte/ && rm -rf build && npm run contentGen && npm run webpGen && svelte-kit build",
		"contentGen": "cd content/timeline && npx m2j . --content -o ../data.json",
		"dev": "svelte-kit dev",
		"export": "npm run build && cp static/resume.pdf build/resume.pdf",
		"lint": "eslint src",
		"start": "svelte-kit start",
		"webpGen": "cd static && webpc -r && cd .."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"vite": "^2.1.0",
		"babel-eslint": "^10.1.0",
		"dateformat": "^4.5.1",
		"eslint": "^7.20.0",
		"eslint-plugin-svelte3": "^3.1.1",
		"markdown-to-json": "^0.5.4",
		"npm-run-all": "^4.1.5",
		"slugify": "^1.3.6",
		"svelte": "^3.31.2",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^4.3.0",
		"webp-converter-cli": "0.0.5"
	},
	"type": "module",
	"engines": {
		"node": ">= 14.15.4"
	}
}
