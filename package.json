  "author": "Ross Hill <ross@rosshill.ca> (https://rosshill.ca)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run getPosts && npm run getIcons && npm run getWebp",
    "build": "npm run clean && svelte-kit build",
    "postbuild": "mv build/404/index.html build/404.html",
    "clean": "rm -rf .svelte/ && rm -rf build",
    "dev": "svelte-kit dev",
    "export": "npm run build",
    "getIcons": "node ./getIcons.js",
    "getPosts": "markdown-json -c markdown-config.json",
    "getWebp": "cd static && webpc -r && cd ..",
    "lint": "eslint src",
    "preview": "svelte-kit preview",
    "start": "svelte-kit start",
    "upgrade": "ncu -u && ncu -u --target newest --filter \"@sveltejs/adapter-static @sveltejs/kit\" && npm i"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@iconify/json": "^1.1.418",
    "@sveltejs/adapter-static": "1.0.0-next.21",
    "@sveltejs/kit": "1.0.0-next.187",
    "color-contrast": "^1.0.0",
    "dateformat": "^5.0.2",
    "eslint": "^8.0.1",
    "eslint-plugin-svelte3": "^3.2.1",
    "isbot": "^3.3.4",
    "markdown-json": "^1.6.0",
    "sass": "^1.43.2",
    "simple-icons": "^5.19.0",
    "slugify": "^1.6.1",
    "svelte": "3.44.0",
    "svelte-elements": "^0.4.2",
    "unplugin-icons": "^0.12.16",
    "vite": "2.6.10",
    "webp-converter-cli": "^0.0.5"
  },
  "engines": {
    "node": ">= 14.15.4"
  }
}
