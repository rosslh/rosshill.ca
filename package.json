{
  "private": true,
  "license": "UNLICENSED",
  "author": "Ross Hill <ross@rosshill.ca> (https://rosshill.ca)",
  "type": "module",
  "scripts": {
    "prebuild": "npm run getPosts && npm run getIcons && npm run getWebp",
    "build": "npm run clean && svelte-kit build",
    "postbuild": "mv build/404/index.html build/404.html",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "clean": "rm -rf .svelte/ && rm -rf build",
    "dev": "svelte-kit dev",
    "export": "npm run build",
    "getIcons": "node --loader ts-node/esm ./getIcons.ts",
    "getPosts": "markdown-json -c markdown-config.json",
    "getWebp": "cd static && webpc -r && cd ..",
    "lint": "eslint . --ignore-path .gitignore",
    "preview": "svelte-kit preview",
    "start": "svelte-kit start",
    "upgrade": "ncu -u && ncu -u --target newest --filter \"@sveltejs/adapter-static @sveltejs/kit\" && npm i"
  },
  "browserslist": "last 5 years or ie>=11 or >0.2%",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@iconify/json": "^1.1.426",
    "@sveltejs/adapter-static": "1.0.0-next.21",
    "@sveltejs/kit": "1.0.0-next.196",
    "@types/chroma-js": "^2.1.3",
    "@types/cookie": "^0.4.1",
    "@types/dateformat": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.3.1",
    "autoprefixer": "^10.4.0",
    "chroma-js": "^2.1.2",
    "cssnano": "^5.0.10",
    "dateformat": "^5.0.2",
    "eslint": "^8.2.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "isbot": "^3.3.4",
    "markdown-json": "^1.6.0",
    "sass": "^1.43.4",
    "simple-icons": "^5.22.0",
    "slugify": "^1.6.2",
    "svelte": "3.44.1",
    "svelte-check": "^2.2.8",
    "svelte-elements": "^0.4.2",
    "svelte-preprocess": "^4.9.8",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "unplugin-icons": "^0.12.18",
    "vite": "2.6.14",
    "webp-converter-cli": "^0.0.5"
  },
  "engines": {
    "node": ">= 14.15.4"
  }
}
