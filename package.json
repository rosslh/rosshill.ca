{
  "name": "homepage",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run projectGen && npm run webpGen && sapper dev",
    "build": "sapper build --legacy",
    "export": "npm run projectGen && npm run webpGen && sapper export --legacy && cp static/resume.pdf __sapper__/export/resume.pdf",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "eslint": "eslint --ext .html,.js --fix src",
    "projectGen": "node node_modules/frontmatter-markdown-to-json --in=content --out=content",
    "webpGen": "cd static && webpc -r && cd .."
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "^0.5.2",
    "sirv": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "chokidar": "^2.0.4",
    "cypress": "^3.1.5",
    "dateformat": "^3.0.3",
    "eslint": "^5.13.0",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "frontmatter-markdown-to-json": "0.0.9",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^1.0.1",
    "sapper": "^0.26.0",
    "slugify": "^1.3.4",
    "svelte": "^2.0.0",
    "svelte-select": "^1.5.2",
    "webp-converter-cli": "0.0.5"
  }
}
