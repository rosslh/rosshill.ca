{
  "author": "Ross Hill <ross@rosshill.ca> (https://rosshill.ca)",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "rm -rf .svelte/ && rm -rf build && npm run contentGen && npm run webpGen && svelte-kit build",
    "postbuild": "mv build/404/index.html build/404.html",
    "contentGen": "markdown-json -c markdown-config.json",
    "dev": "svelte-kit dev",
    "export": "npm run build",
    "lint": "eslint src",
    "preview": "svelte-kit preview",
    "start": "svelte-kit start",
    "upgrade": "ncu -u && ncu -u --target newest --filter \"@sveltejs/adapter-static @sveltejs/kit\" && npm i",
    "webpGen": "cd static && webpc -r && cd .."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.19",
    "@sveltejs/kit": "1.0.0-next.169",
    "babel-eslint": "^10.1.0",
    "dateformat": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "isbot": "^3.3.3",
    "markdown-json": "^1.6.0",
    "sass": "^1.41.1",
    "slugify": "^1.6.0",
    "svelte": "3.42.6",
    "svelte-icons": "^2.1.0",
    "vite": "2.5.10",
    "webp-converter-cli": "^0.0.5"
  },
  "engines": {
    "node": ">= 14.15.4"
  }
}
