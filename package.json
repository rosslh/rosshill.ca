{
  "name": "homepage",
  "version": "1.2.0",
  "description": "Ross Hill's homepage",
	"scripts": {
		"build": "rm -rf .svelte/ && rm -rf build && npm run contentGen && npm run webpGen && svelte-kit build",
		"contentGen": "cd content/timeline && npx m2j . --content -o ../data.json",
		"dev": "svelte-kit dev",
		"export": "npm run build && cp static/resume.pdf build/resume.pdf",
		"lint": "eslint src",
		"start": "svelte-kit start",
		"webpGen": "cd static && webpc -r && cd .."
	},
	"dependencies": {
		"core-js": "^3.9.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"vite": "^2.1.0",
		"@babel/core": "^7.11.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.13.8",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/plugin-transform-spread": "^7.13.0",
		"@babel/preset-env": "^7.11.5",
		"@babel/runtime": "^7.11.2",
		"@rollup/plugin-babel": "^5.2.0",
		"@rollup/plugin-commonjs": "^15.1.0",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^11.2.1",
		"@rollup/plugin-replace": "^2.3.3",
		"autoprefixer": "^9.8.6",
		"babel-eslint": "^10.1.0",
		"cssnano": "^4.1.10",
		"dateformat": "^3.0.3",
		"eslint": "^7.20.0",
		"eslint-plugin-svelte3": "^3.1.1",
		"markdown-to-json": "^0.5.4",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.26.11",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-svelte": "^6.0.1",
		"rollup-plugin-terser": "^5.3.0",
		"slugify": "^1.3.6",
		"svelte": "^3.31.2",
		"svelte-icons": "^1.1.0",
		"svelte-preprocess": "^4.3.0",
		"webp-converter-cli": "0.0.5"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	}
}
