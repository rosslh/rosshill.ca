{
  "author": "Ross Hill <ross@rosshill.ca> (https://rosshill.ca)",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "build": "rm -rf .svelte/ && rm -rf build && npm run contentGen && npm run iconGen && npm run webpGen && svelte-kit build",
    "contentGen": "markdown-json -c markdown-config.json",
    "dev": "svelte-kit dev",
    "export": "npm run build",
    "iconGen": "node ./getIcons.js",
    "lint": "eslint src",
    "postbuild": "mv build/404/index.html build/404.html",
    "preview": "svelte-kit preview",
    "start": "svelte-kit start",
    "upgrade": "ncu -u && ncu -u --target newest --filter \"@sveltejs/adapter-static @sveltejs/kit\" && npm i",
    "webpGen": "cd static && webpc -r && cd .."
  },
  "devDependencies": {
    "@iconify/json": "^1.1.413",
    "@sveltejs/adapter-static": "1.0.0-next.21",
    "@sveltejs/kit": "1.0.0-next.183",
    "babel-eslint": "^10.1.0",
    "color-contrast": "^1.0.0",
    "dateformat": "^5.0.1",
    "eslint": "^8.0.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "isbot": "^3.3.4",
    "markdown-json": "^1.6.0",
    "sass": "^1.42.1",
    "simple-icons": "^5.18.0",
    "slugify": "^1.6.1",
    "svelte": "3.43.2",
    "svelte-elements": "^0.4.2",
    "unplugin-icons": "^0.12.14",
    "vite": "2.6.7",
    "webp-converter-cli": "^0.0.5"
  },
  "engines": {
    "node": ">= 14.15.4"
  }
}
